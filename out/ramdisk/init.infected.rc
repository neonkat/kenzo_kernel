on boot

on property:sys.boot_completed=1

    # ondemand
    
    # KSM

    write /sys/kernel/mm/ksm/run 0
    write /sys/kernel/mm/ksm/pages_to_scan 100
    write /sys/kernel/mm/ksm/sleep_millisecs 500

	
    #FUCK THIS BITCH 

    write /sys/module/msm_thermal/core_control/enabled 0
    write /sys/module/msm_performance/parameters/touchboost 0

    #Arch power

    write /sys/kernel/sched/arch_power 1
   
   # change cpu min frequency
   
    chmod 0664 /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 400000
    chmod 0664 /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq 400000

    # i/o tweaks
    write /sys/block/mmcblk0/queue/scheduler maple

    # idle freq
    write /sys/devices/system/cpu/cpu0/sched_mostly_idle_freq 1017600
    write /sys/devices/system/cpu/cpu4/sched_mostly_idle_freq 0

    #CPU Boost

    write /sys/module/cpu_boost/parameters/input_boost_enabled 1
    write /sys/module/cpu_boost/parameters/input_boost_freq "0:1017600 1:0 2:0 3:0 4:940800 5:0"
    write /sys/module/cpu_boost/parameters/input_boost_ms 40
    write /sys/module/cpu_boost/parameters/migration_load_threshold 15

    # turn on bitches

    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_governor "interactive"
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_governor "interactive"
    write /sys/module/workqueue/parameters/power_efficient Y
    

    # cafactive(initially will run on interactive loads,will change in future)

    --small--
    write /sys/devices/system/cpu/cpu0/cpufreq/cafactive/go_hispeed_load 75
    write /sys/devices/system/cpu/cpu0/cpufreq/cafactive/above_hispeed_delay 35000
    write /sys/devices/system/cpu/cpu0/cpufreq/cafactive/hispeed_freq 1017600
    write /sys/devices/system/cpu/cpu0/cpufreq/cafactive/io_is_busy 0
    write /sys/devices/system/cpu/cpu0/cpufreq/cafactive/min_sample_time 45000
    write /sys/devices/system/cpu/cpu0/cpufreq/cafactive/timer_rate 33000
    write /sys/devices/system/cpu/cpu0/cpufreq/cafactive/max_freq_hysteresis 100000
    write /sys/devices/system/cpu/cpu0/cpufreq/cafactive/timer_slack 100000
    write /sys/devices/system/cpu/cpu0/cpufreq/cafactive/target_loads "65 883200:35 1017600:55 1190400:65 1305600:75 1401600:85 1440000:99"

    --big--
    write /sys/devices/system/cpu/cpu4/cpufreq/cafactive/go_hispeed_load 70
    write /sys/devices/system/cpu/cpu4/cpufreq/cafactive/hispeed_freq 1248000
    write /sys/devices/system/cpu/cpu4/cpufreq/cafactive/min_sample_time 30000
    write /sys/devices/system/cpu/cpu4/cpufreq/cafactive/use_sched_load 0
    write /sys/devices/system/cpu/cpu4/cpufreq/cafactive/target_loads "65 883200:25 998400:30 1056000:43 1190400:53 1305600:65 1612800:75 1747200:83 1804800:90"
    
	
    # interactive

   --small--
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_min_freq 400000
    write /sys/devices/system/cpu/cpu0/cpufreq/scaling_max_freq 1440000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_min_freq 400000
    write /sys/devices/system/cpu/cpu4/cpufreq/scaling_max_freq 1958600
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/go_hispeed_load 99
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/above_hispeed_delay 25000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/hispeed_freq 1440000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/io_is_busy 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/min_sample_time 45000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_rate 35000
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/max_freq_hysteresis 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/timer_slack -1
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/target_loads "60 806400:35 1017600:55 1190400:70 1305600:80 1401600:90 1440000:99"   
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/use_sched_load 0
    write /sys/devices/system/cpu/cpu0/cpufreq/interactive/align_windows 1
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/timer_slack -1
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/go_hispeed_load 65
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/above_hispeed_delay "15000 1382400:35000"
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/hispeed_freq 1382400
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/min_sample_time 50000
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/use_sched_load 0
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/align_windows 1
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/target_loads "50 883200:30 998400:45 1113600:55 1382400:65 1612800:75 1747200:85 1804800:93 1958600:98"
    write /sys/devices/system/cpu/cpu4/cpufreq/interactive/max_freq_hysteresis 45000
    
    # cultivation 

    --small--
    write /sys/devices/system/cpu/cpu0/cpufreq/cultivation/go_hispeed_load 99
    write /sys/devices/system/cpu/cpu0/cpufreq/cultivation/above_hispeed_delay 25000
    write /sys/devices/system/cpu/cpu0/cpufreq/cultivation/hispeed_freq 1440000
    write /sys/devices/system/cpu/cpu0/cpufreq/cultivation/min_sample_time 40000
    write /sys/devices/system/cpu/cpu0/cpufreq/cultivation/timer_rate 35000
    write /sys/devices/system/cpu/cpu0/cpufreq/cultivation/max_freq_hysteresis 35000
    write /sys/devices/system/cpu/cpu0/cpufreq/cultivation/timer_slack -1
    write /sys/devices/system/cpu/cpu0/cpufreq/cultivation/go_lowspeed_load 15
    write /sys/devices/system/cpu/cpu0/cpufreq/cultivation/timer_slack -1
    write /sys/devices/system/cpu/cpu0/cpufreq/cultivation/target_loads "60 806400:35 1017600:55 1190400:70 1305600:80 1401600:90 1440000:99"

    # blu_active
    
    --small--
    write /sys/devices/system/cpu/cpu0/cpufreq/blu_active/go_hispeed_load 99
    write /sys/devices/system/cpu/cpu0/cpufreq/blu_active/above_hispeed_delay 25000
    write /sys/devices/system/cpu/cpu0/cpufreq/blu_active/hispeed_freq 1440000
    write /sys/devices/system/cpu/cpu0/cpufreq/blu_active/min_sample_time 40000
    write /sys/devices/system/cpu/cpu0/cpufreq/blu_active/timer_rate 35000
    write /sys/devices/system/cpu/cpu0/cpufreq/blu_active/timer_slack -1
    write /sys/devices/system/cpu/cpu0/cpufreq/blu_active/fastlane 1
    write /sys/devices/system/cpu/cpu0/cpufreq/blu_active/fastlane_threshold 70
    write /sys/devices/system/cpu/cpu4/cpufreq/blu_active/target_loads "50 1017600:60"
    

    # conservative

    write /sys/devices/system/cpu/cpu0/cpufreq/conservative/freq_step 9
    write /sys/devices/system/cpu/cpu4/cpufreq/conservative/freq_step 12
	
    # despair

    write /sys/devices/system/cpu/cpu0/cpufreq/despair/freq_step 9
    write /sys/devices/system/cpu/cpu4/cpufreq/despair/freq_step 12
	
    # bioshock

    write /sys/devices/system/cpu/cpu0/cpufreq/bioshock/freq_step 9
    write /sys/devices/system/cpu/cpu4/cpufreq/bioshock/freq_step 12

    #relaxed

    write /sys/devices/system/cpu/cpu0/cpufreq/relaxed/freq_step 9
    write /sys/devices/system/cpu/cpu4/cpufreq/relaxed/freq_step 12

    # process_reclaim

    write /sys/module/process_reclaim/parameters/enable_process_reclaim 1
    write /sys/module/process_reclaim/parameters/pressure_max 80
	
start infected

service infected /system/bin/sh /infected.sh
    class main
    user root
    disabled
    oneshot
